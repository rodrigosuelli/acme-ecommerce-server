{
  "kind": "collectionType",
  "collectionName": "produtos",
  "info": {
    "singularName": "produto",
    "pluralName": "produtos",
    "displayName": "Produto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "descricao": {
      "type": "text",
      "required": true
    },
    "imagens": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "preco_real": {
      "type": "decimal",
      "required": true
    },
    "categorias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categoria.categoria",
      "mappedBy": "produtos"
    },
    "preco_original": {
      "type": "decimal"
    },
    "qtd_estoque": {
      "required": true,
      "type": "integer"
    },
    "ativo": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo",
      "required": true
    },
    "tipos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tipo.tipo",
      "mappedBy": "produtos"
    },
    "avaliacao": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5,
      "default": 5
    },
    "qtd_avaliacoes": {
      "type": "integer",
      "required": true
    }
  }
}
